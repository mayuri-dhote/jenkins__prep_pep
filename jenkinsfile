pipeline {
     agent {
        label 'jenkins'
    }

    stages{

        stage ('build') {
            steps {
                sh 'mvn compile'
            }
        
             }
        stage ('test') {
            steps {
                sh 'mvn clean test'
            }
        }
        stage ('package') {
            steps {
                sh 'mvn clean package'
            }
        }


    }
    post {
    success {
      sh 'echo "The Pipeline has passed"'
    }
    failure {
        sh 'echo "The Pipeline has failed"'
    }
  }
